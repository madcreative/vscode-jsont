{
    "name": "json-template",
    "displayName": "JSON-Template",
    "description": "JSON-Template / Squarespace language support for Visual Studio Code.",
    "icon": "images/squarespace-white.png",
    "galleryBanner": {
        "color": "#222",
        "theme": "dark"
    },
    "version": "0.2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/stormwarning/vscode-jsont.git"
    },
    "publisher": "stormwarning",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "jsont",
                "aliases": [
                    "json template",
                    "jsont",
                    "squarespace",
                    "sqsp"
                ],
                "extensions": [
                    ".block",
                    ".item",
                    ".list",
                    ".page",
                    ".region"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "jsont",
                "scopeName": "text.html.jsont",
                "path": "./syntaxes/jsont.tmLanguage"
            }
        ]
    }
}